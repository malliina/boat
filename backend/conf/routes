GET        /                                controllers.BoatController.index
GET        /pingAuth                        controllers.BoatController.pingAuth
GET        /health                          controllers.BoatController.health
GET        /tracks                          controllers.BoatController.tracks
GET        /tracks/:track                   controllers.BoatController.summary(track: com.malliina.boat.TrackName)
GET        /tracks/:track/trail             controllers.BoatController.track(track: com.malliina.boat.TrackName)
GET        /ws/updates                      controllers.BoatController.updates
GET        /ws/boats                        controllers.BoatController.boats
GET        /distances                       controllers.BoatController.distances
GET        /sign-in/google                  controllers.Social.google
GET        /sign-in/callbacks/google        controllers.Social.googleCallback

GET        /files                           controllers.FileController.list
GET        /files/*file                     controllers.FileController.download(file)
GET        /assets/*file                    controllers.BoatController.versioned(path = "/public", file: Asset)
