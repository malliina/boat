POST         /users/notifications/disable        controllers.PushController.disableNotifications
POST         /boats                              controllers.BoatController.createBoat
PATCH        /boats/:boat                        controllers.BoatController.renameBoat(boat: com.malliina.boat.BoatId)
GET          /tracks                             controllers.BoatController.tracks
GET          /tracks/:track                      controllers.BoatController.track(track: com.malliina.boat.TrackName)
GET          /tracks/:track/trail                controllers.BoatController.trail(track: com.malliina.boat.TrackName)
GET          /tracks/:track/full                 controllers.BoatController.full(track: com.malliina.boat.TrackName)
GET          /tracks/:track/chart                controllers.BoatController.chart(track: com.malliina.boat.TrackName)
GET          /ws/updates                         controllers.BoatController.updates
GET          /ws/boats                           controllers.BoatController.boats
GET          /distances                          controllers.BoatController.distances
